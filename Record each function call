def fun3():
    pass

def fun2():
    fun3()

def func1():
    fun2()

def func(frame, event, arg, indent=[0]):
      if event == "call":
          indent[0] += 2
          print ">> " * indent[0] + "CALLING: {0}".format(frame.f_code.co_name)
      elif event == "return":
          print "<< " * indent[0], "RETURNING: {0}".format(frame.f_code.co_name.strip())
          indent[0] -= 2
      return func

import sys
sys.settrace(func)

func1()
